<%= form_tag(search_registers_path, :method => 'get', :class => 'form-inline') do |f| %>
  <%= label_tag(:start, "De:") %>
  <%= text_field_tag(:start, l(@navigator.start_date), class: "datepicker") %>
  <%= label_tag(:finish, "Hasta:") %>
  <%= text_field_tag(:finish, l(@navigator.finish_date), class: "datepicker") %>
  <%= submit_tag("Buscar", class:"btn") %>
<% end %>